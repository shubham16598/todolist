    "devDependencies": {
        "nodemon": "^2.0.4"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "concurrently": "^5.2.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "mongoose": "^5.9.21"
    }
}

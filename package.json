{
    "name": "todo_app_db",
    "version": "1.0.0",
    "description": "Todo App",
    "main": "server.js",
    "engines": {
        "node": "12.14.0"
    },
    "scripts": {
        "client-install": "npm install --prefix client",
        "start": "node server.js",
        "server": "nodemon server.js",
